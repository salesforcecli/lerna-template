{
  "name": "@salesforce/plugin-template",
  "version": "1.0.0",
  "description": "A template plugin that lives in a lerna project",
  "author": "Salesforce",
  "homepage": "https://github.com/salesforcecli/lerna-template/tree/master/packages/plugin-template#readme",
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    }
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salesforcecli/lerna-template.git"
  },
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "sf-build",
    "pretest": "sf-compile-test",
    "test": "sf-test"
  },
  "dependencies": {
    "@oclif/config": "^1",
    "@salesforce/command": "^3.0.3",
    "@salesforce/core": "^2.13.0",
    "@salesforce/template-lib": "^1.0.0",
    "@salesforce/ts-types": "^1.4.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@salesforce/dev-config": "^2.0.0",
    "@salesforce/dev-scripts": "^0.7.0",
    "@salesforce/prettier-config": "^0.0.1",
    "@salesforce/ts-sinon": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-salesforce": "^0.1.0",
    "eslint-config-salesforce-license": "^0.1.0",
    "eslint-config-salesforce-typescript": "^0.2.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^31.6.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "shx": "^0.3.3",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
}
